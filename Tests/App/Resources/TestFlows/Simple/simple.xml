<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:camel="http://camel.apache.org/schema/spring"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">

  <camelContext trace="false" xmlns="http://camel.apache.org/schema/spring">
    <route>
        <from uri="test://smartesb/simple1"/>
        <to uri="helper://smartesb/multiply/5"/>
    </route>
    <route>
        <from uri="test://smartesb/simple2"/>
        <to uri="helper://smartesb/multiply/2"/>
    </route>
    <route>
        <from uri="test://smartesb/simple3"/>
        <to uri="helper://smartesb/add/5"/>
    </route>
    <route>
        <from uri="test://smartesb/simple4"/>
        <to uri="helper://smartesb/add/5"/>
        <to uri="helper://smartesb/multiply/2"/>
    </route>
</camelContext>

</beans>
